{
  "name": "react-ssr-setup",
  "version": "1.6.0",
  "author": "Manuel Bieh <node@manuelbieh.de> (https://github.com/manuelbieh)",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0",
    "yarn": "^1.3.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "start": "node build/server/server.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "dev": "cross-env NODE_ENV=development node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project ./tsconfig.json --config ./tslint.json -e \"**/build/**\"",
    "test:update": "yarn test --updateSnapshot",
    "ganache": "ganache-cli -b 5",
    "truffle": "cross-env NODE_ENV=development truffle exec ./scripts/init-truffle.js && truffle console"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --config ./.prettierrc --config-precedence file-override",
      "git add"
    ]
  },
  "moduleRoots": [
    "src/client",
    "src/server",
    "src/shared",
    "src",
    "node_modules"
  ],
  "dependencies": {
